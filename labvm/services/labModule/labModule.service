[Unit]
Description=Automatically configures CVP with EOS devices depending on the topology
Required=cvpUpdater.service
After=cvpUpdater.service

[Service]
Type=forking
ExecStart=/usr/local/bin/labModule.py
User=arista
TimeoutStartSec=900
Restart=on-failure
RestartSec=60

[Install]
WantedBy=multi-user.target
